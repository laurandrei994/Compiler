#ifndef QUEX_INCLUDE_GUARD_quexProcessed__LEXEME__BASICS
#define QUEX_INCLUDE_GUARD_quexProcessed__LEXEME__BASICS



/* HINT: If compiler complaints about non-existing 'lexeme_length' function,
 *       then this is probably caused by a user-defined token class file which
 *       does not include 'quex/code_base/lexeme.i'                           */
inline size_t 
quexProcessed_lexeme_length(const quexProcessed_lexatom_t* Str);


inline quexProcessed_lexatom_t*
quexProcessed_lexeme_clone(const quexProcessed_lexatom_t* BeginP,
                        size_t                   Length);

inline size_t 
quexProcessed_lexeme_compare(const quexProcessed_lexatom_t* it0, 
                          const quexProcessed_lexatom_t* it1);



#endif /* QUEX_INCLUDE_GUARD_quexProcessed__LEXEME__BASICS */
